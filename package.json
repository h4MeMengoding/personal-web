{
  "name": "sznm.dev",
  "private": true,
  "description": "Agustinus Nathaniel's dev site",
  "version": "1.26.2",
  "author": "sozonome",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "prisma generate",
    "dev": "next dev",
    "build": "contentlayer build && next build",
    "build:turbo": "npx turbo build",
    "analyze": "cross-env ANALYZE=true pnpm build",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "test": "vitest",
    "test:ui": "pnpm test --ui",
    "test:run": "pnpm test run",
    "test:coverage": "pnpm test:run --coverage",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && pnpm format",
    "type-check": "tsc --noEmit",
    "knip": "knip",
    "check:turbo": "npx turbo lint type-check",
    "format": "prettier --write src",
    "up-interactive": "pnpm up -i",
    "up-latest": "pnpm up-interactive -L",
    "release": "cross-env HUSKY=0 standard-version",
    "push-release": "git push --follow-tags origin main",
    "db:studio": "npx prisma studio",
    "prepare": "husky install"
  },
  "dependencies": {
    "@chakra-ui/next-js": "^2.1.5",
    "@chakra-ui/react": "2.8.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@giscus/react": "^2.3.0",
    "@prisma/client": "^5.5.2",
    "@t3-oss/env-nextjs": "^0.7.1",
    "@vercel/speed-insights": "^1.0.1",
    "cmdk": "^0.2.0",
    "contentlayer": "^0.3.4",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.4",
    "lodash-es": "^4.17.21",
    "million": "2.6.4",
    "next": "14.0.1",
    "next-contentlayer": "^0.3.4",
    "next-seo": "^6.4.0",
    "nextjs-toploader": "^1.6.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.11.0",
    "react-markdown": "^8.0.7",
    "react-syntax-highlighter": "^15.5.0",
    "react-wrap-balancer": "^1.1.0",
    "reading-time": "^1.5.0",
    "remark-gfm": "^3.0.1",
    "rss": "^1.2.2",
    "sass": "^1.69.5",
    "swr": "^2.2.4",
    "ua-parser-js": "^1.0.37",
    "zod": "^3.22.4",
    "zustand": "^4.4.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "18.1.0",
    "@commitlint/cz-commitlint": "18.2.0",
    "@next/bundle-analyzer": "^14.0.1",
    "@types/lodash-es": "^4.17.10",
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.33",
    "@types/react-syntax-highlighter": "^15.5.9",
    "@types/rss": "^0.0.31",
    "@types/ua-parser-js": "^0.7.38",
    "@vitest/coverage-c8": "^0.33.0",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.52.0",
    "eslint-config-next": "14.0.1",
    "eslint-config-sznm": "^2.0.2",
    "eslint-plugin-no-relative-import-paths": "^1.5.3",
    "husky": "^8.0.3",
    "knip": "^2.38.5",
    "lint-staged": "^15.0.2",
    "next-sitemap": "^4.2.3",
    "prettier": "^3.0.3",
    "prisma": "^5.5.2",
    "standard-version": "^9.5.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sozonome/sznm.dev"
  },
  "bugs": {
    "url": "https://github.com/sozonome/sznm.dev/issues"
  },
  "engines": {
    "node": ">=18.x",
    "pnpm": ">=8"
  }
}
